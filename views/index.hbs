<section id="contact" class="contact">
    <div class="container">
        <div class="row mb50">
            <div class="sec-title text-center">
                <h1>Contacto</h1>
            </div>
            <div class="sec-sub-title text-center">
            </div>
            <!-- contact address -->
            <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12 wow fadeInLeft animated" data-wow-duration="2s">
                <div class="contact-address">
                    <h3>Flor Paramidani</h3>
                    <h4>Diseñador Gráfico</h4>
                    <h4>Frontend Developer</h4>
                    <h4>Buenos Aires, Argentina</h4>
                    <h4>florparamidani@gmail.com</h4>
                </div>
            </div>
            <!-- end contact address -->

            <!-- contact form -->
            <div class="col-lg-8 col-md-8 col-sm-7 col-xs-12">
                <div class="contact-form">
                    <h3>Dudas, consultas? Dejame un mensaje y lo responderé a la brevedad:</h3>
                    <form action="/" method="post">
                        <div class="input-group name-email">
                            <div class="input-field">
                                <input type="text" id="name" name="name" placeholder="name" class="form-control">
                            </div>
                            <div class="input-field">
                                <input type="email" id="email" name="email" placeholder="email" class="form-control">
                            </div>
                        </div>
                        <div class="input-group">
                            <textarea id="message" name="message" placeholder="message" class="form-control"></textarea>
                        </div>
                        <div class="input-group">
                            <input type="submit" id="form-submit" class="pull-right" value="Send message">
                        </div>
                    </form>
                    <p>{{message}}</p>
                </div>
            </div>
            <!-- end contact form -->

            <!-- footer social links -->
            <div class="col-lg-1 col-md-1 col-sm-1 col-xs-12 wow fadeInRight animated" data-wow-duration="500ms"
                data-wow-delay="600ms">
                <ul class="footer-social">
                    <li><a href="https://twitter.com/ParamidaniFlor"><i class="fa-brands fa-twitter"></i></a></li>
                    <li><a href="https://www.instagram.com/florpdg/"><i class="fa-brands fa-instagram"></i></a></li>

                </ul>
            </div>
            <!-- end footer social links -->

        </div>
    </div>
</section>